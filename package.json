    "prettier": "^3.6.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-grid-layout": "^1.5.3",
    "replace-in-file-webpack-plugin": "^1.0.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.0",
    "semver": "^7.7.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.0",
    "terser-webpack-plugin": "^5.3.0",
