    "css-element-queries": "^1.2.0",
    "eventemitter3": "2.0.3",
    "pako": "2.1.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-sizeme": "3.0.2",
    "three": "0.182.0"
  },
  "engines": {
    "node": ">= 22 <25"
