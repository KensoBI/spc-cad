{
  "name": "kensobi-spccad-panel",
  "version": "3.0.0",
  "description": "Create interactive 3D dashboards with CAD models, annotations and features.",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cad",
    "kensobi",
    "kenso",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Kenso Software",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.1.2",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.57.0",
    "@popperjs/core": "2.11.5",
    "@stylistic/eslint-plugin-ts": "^4.4.0",
    "@swc/core": "^1.15.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "4.14.182",
    "@types/node": "^24.0.0",
    "@types/pako": "2.0.0",
    "@types/react": "^18.3.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-grid-layout": "1.3.5",
    "@types/scriptjs": "0.0.2",
    "@types/three": "0.143.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.0",
    "eslint": "^9.32.0",
